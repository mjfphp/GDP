schema {
    query: Query
    mutation: Mutation
}
type Query {
    allSections: [Section]
}

type Section {
    id: ID!
    intitule: String
    taux: Int
}
type Mutation {
    createSection(intitule: String!, taux: Int!): Section
    deleteSection(id: String!): Section
    updateSection(id: String!,intitule: String!,taux: Int!): Section
}